{
  "compilerOptions": {
    "target": "es5",
    "lib": ["es2015.promise", "es5"],
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "removeComments": false,
    "noImplicitReturns": true,
    "noImplicitAny": false,
    "preserveConstEnums": true,
    "allowJs": true
  },
  "exclude": [
    "logs",
    "node_modules"
  ]
}